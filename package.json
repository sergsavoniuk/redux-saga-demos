{
  "name": "redux-saga-demos",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergsavoniuk/redux-saga-demos.git"
  },
  "dependencies": {
    "connected-react-router": "^6.3.2",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src/ react-scripts start",
    "build-sw": "node ./src/sw-build.js",
    "build": "cross-env NODE_PATH=src/ react-scripts build && npm run build-sw",
    "build:run": "npm run build && node ./src/server.js",
    "format": "prettier \"src/**/*.{js,jsx}\" --write",
    "test": "cross-env NODE_PATH=src/ react-scripts test --verbose false",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/constants/"
    ]
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "rimraf": "^2.6.3",
    "workbox-build": "^4.3.1"
  }
}
