    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
